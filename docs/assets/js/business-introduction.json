{
    "showProgressBar": "belowHeader",
    "progressBarType": "pages",
    "progressBarShowPageNumbers": true,
    "pages": [
      {
        "name": "Introduction",
        "elements": [
          {
            "type": "html",
            "name": "introText",
            "html": "<h2>Introduction to the Business Value of Automated Testing</h2><p>In this section, we will explore the business value of automated testing and discuss key concepts and questions that will help you understand its importance. This discussion-focused module will guide you through the maturity model of an organization, existing investments in automated testing, and business motivators.</p>"
          }
        ]
      },
      {
        "name": "Maturity",
        "elements": [
          {
            "type": "radiogroup",
            "name": "maturityStage",
            "title": "What is the current maturity stage of your organization in terms of automated testing?",
            "choices": [
              "Initial Stage",
              "Developing Stage",
              "Mature Stage"
            ]
          }
        ]
      },
      {
        "name": "Challenges",
        "elements": [
          {
            "type": "checkbox",
            "name": "initialStageChallenges",
            "visibleIf": "{maturityStage} = 'Initial Stage'",
            "title": "What are the challenges and limitations of manual testing in your organization?",
            "choices": [
              "Time-consuming",
              "Prone to human error",
              "Difficult to scale",
              "Inconsistent results",
              "High cost",
              "Other (please specify)"
            ],
            "hasOther": true
          }
        ]
      },
      {
        "name": "Impact",
        "elements": [
          {
            "type": "checkbox",
            "name": "developingStageImpact",
            "visibleIf": "{maturityStage} = 'Developing Stage'",
            "title": "How does the transition to automated testing impact your team's efficiency and effectiveness?",
            "choices": [
              "Increased speed of testing",
              "Improved accuracy",
              "Better resource allocation",
              "Enhanced test coverage",
              "Reduced costs",
              "Other (please specify)"
            ],
            "hasOther": true
          }
        ]
      },
      {
        "name": "Benefits",
        "elements": [
          {
            "type": "checkbox",
            "name": "matureStageBenefits",
            "visibleIf": "{maturityStage} = 'Mature Stage'",
            "title": "What are the benefits of reaching this level of maturity, and how does it contribute to business goals?",
            "choices": [
              "Higher quality products",
              "Faster time to market",
              "Increased customer satisfaction",
              "Greater innovation",
              "Sustainable processes",
              "Other (please specify)"
            ],
            "hasOther": true
          }
        ]
      },
