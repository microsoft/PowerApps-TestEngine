{
"version": "0.2.0",
"configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false,
            "symbolOptions": {
              "searchMicrosoftSymbolServer": true,
              "searchPaths": [
                "${workspaceFolder}/../bin/Debug/PowerAppsTestEngine",
                "${workspaceFolder}/../bin/Debug/PowerAppsTestEngine"
              ],
              "moduleFilter": {
                  "mode": "loadOnlyIncluded",
                  "includedModules": ["*.dll"]
              }
            }
        }
    ]
}